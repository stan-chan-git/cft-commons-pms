<?xml version="1.0" encoding="UTF-8"?>
<decorators defaultdir="/WEB-INF/layouts/">
	<excludes>
		<pattern>/static/*</pattern>
		<pattern>/uams/login*</pattern>
		<pattern>/uams/logout*</pattern>
		<pattern>/uams/view/toCreateUser*</pattern>
		<pattern>/uams/view/toUpdateUser*</pattern>
		<pattern>/view/createUser*</pattern>
		<pattern>/view/createRole*</pattern>
		<pattern>/view/createRecord*</pattern>
		<pattern>/uams/registered*</pattern>
		
	</excludes>
	
	<decorator name="default" page="default.jsp">
		<pattern>/*</pattern>
	</decorator>
</decorators>
